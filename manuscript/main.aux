\@writefile{lof}{\contentsline {figure}{\numberline {10}{\ignorespaces \textbf  {Expansion et BAO.} Ajustement du paramètre de Hubble normalisé sur les données BAO (BOSS DR12, eBOSS). Le modèle MCGT passe précisément par les points de données Lyman-$\alpha $ à haut redshift ($z \approx 2.3$).}}{5}{figure.caption.18}\protected@file@percent }
\@writefile{lof}{\contentsline {figure}{\numberline {11}{\ignorespaces \textbf  {L'Horizon Sonore ($r_s$).} Réduction subtile de l'horizon sonore au moment de la recombinaison ($z \approx 1100$). Cette réduction géométrique compense l'augmentation locale de $H_0$.}}{5}{figure.caption.20}\protected@file@percent }
